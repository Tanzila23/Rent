	function Stockfigure() {
		$.ajax({
			type: "POST",
			url: "/Home/Get_Dashboard_Stockfigure",
			contentType: "application/json; charset=utf-8",
			dataType: "json",
			//data: JSON.stringify({
			//	ART_CODE: artcode
			//}),
			success: function (response) {
				console.log(response);

				if (response) {
					$("#Total_Inventory").html(numberWithCommas(parseInt(response[0].FULL_AMOUNT)));
					$("#Total_Sale").html(response[0].STOCK_MARGIN + " %");
				}
			},
			error: function (errormessage) {
				alert(errormessage.responseText);
			}
		});
	}


