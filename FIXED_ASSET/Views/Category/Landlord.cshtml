            }),
            success: function (response) {
                console.log(response);
                if (response == '') {
                    $('#BankName_0').val();
                    $('#bankdistrict_edit').val();
                    // $('#BranchName_').val();
                    $('#BranchName_0').val();
                    $("#AccHolder_0").val();
                    $("#AccNumber_0").val();
                    $("#Currency_0").val();
                    $("#Primary_0").val();
                }
                else {

                }
                //console.log(response.data.DISTRICT);

                $('#BankName_0').val(response.data.Bankk_ID).trigger('change');
                // $('#BankDistrict_0').val(response.data.DISTRICT).trigger('change');
                // $('#bankdistrict_edit').val(response.data.DISTRICT).trigger('change');
                DistrictListReload(0, response.data.Bankk_ID, response.data.DISTRICT);
                // $('#branch_edit').val(response.data.Bankk_ID, response.data.DISTRICT, response.data.BRANCH);
                BranchListReload(0, response.data.Bankk_ID, response.data.DISTRICT, response.data.BRANCH);
                //alert(response.data.Bankk_ID, response.data.DISTRICT, response.data.BRANCH);
                //$('#branch_edit').val(response.data.BRANCH);
                $("#AccHolder_0").val(response.data.ACC_NAME);
                $("#AccNumber_0").val(response.data.ACC_NUMBER);
                $("#Currency_0").val(response.data.CURRENCY);
                $("#Primary_0").val(response.data.PRIMARY);

                if (response.data.STATUS == 'Active') {
                    $("#Status_").val('true');
                } else {
                    $("#Status_").val('false');
                }

                $("#btnUpdate").show();
                function DistrictListReload(r_id, vBankID, district_edit) {
                    $.ajax({
                        url: '/AssetCategory/DistrictList',
                        type: 'POST',
                        contentType: "application/json;charset=utf-8",
                        data: JSON.stringify(
                            {
                                Bank_ID: vBankID
                            }
                        ),
                        dataType: "json",
                        success: function (response) {

                            if (response.length > 0) {
                                $('#BankDistrict_' + r_id).html('');
                                var result = '';
                                result += "<option value=''>SELECT ONE</option>";
                                var DistrictEdit = district_edit;
                                for (var i = 0; response.length > i; i++) {
                                    if (response[i].District_Code == $('#bankdistrict_edit').val()) {
                                        //alert($('#bankdistrict_edit').val());
                                        //alert(response[i].District_Code);
                                        result += "<option selected value='" + response[i].District_Code + "' data-shortname='" + response[i].District_Code + "'>" + response[i].District_Name + "</option>";
                                    } else {
                                        result += "<option value='" + response[i].District_Code + "' data-shortname='" + response[i].District_Code + "'>" + response[i].District_Name + "</option>";
                                    }
                                }
                                $('#BankDistrict_' + r_id).append(result);
                                $('#BankDistrict_' + r_id).val(DistrictEdit);
                                $("#BankDistrict_" + r_id).select2();
                            }
                        }
                    });
                }

                function BranchListReload(r_id, vBankID, vDistrictID, branch_edit) {

                    $.ajax({
                        url: '/AssetCategory/BranchList',
                        type: 'POST',
                        contentType: "application/json;charset=utf-8",
                        data: JSON.stringify(
                            {
                                Bank_ID: vBankID,
                                District_Code: vDistrictID
                            }
                        ),
                        dataType: "json",
                        success: function (response) {

                            if (response.length > 0) {
                                $('#BranchName_' + r_id).html('');
                                var result = '';
                                result += "<option value=''>SELECT ONE</option>";
                                var BranchEdit = branch_edit;
                                // alert(BranchEdit);
                                for (var i = 0; response.length > i; i++) {

                                    result += "<option selected value='" + response[i].Branch_Code + "' data-shortname='" + response[i].Branch_Code + "'>" + response[i].Branch_Name + "</option>";

                                }
                                $('#BranchName_' + r_id).append(result);
                                if (r_id == '') {
                                    $('#BranchName_' + r_id).val(BranchEdit);
                                }
                                else {
                                    $('#BranchName_0' + r_id).val(BranchEdit);
                                }

                                $("#BranchName_" + r_id).select2();

                            }
                        }
                    });
                }

            }

        })


    }
