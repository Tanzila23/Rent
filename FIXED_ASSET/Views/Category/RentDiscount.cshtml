                                        <tr>
                                            <th id="Start_Date">Expected Rent Pay Day</th>
                                            @*<th id="End_Date">Year</th>*@
                                            <th id="Increment_Amount">Basic Rent</th>
                                            <th id="Increment_VAT">Tax Based Amount</th>

                                        </tr>
                                        @*<tr>
                            <td style="width:300px" data-bind="First_start_date">

                            </td>
                        </tr>*@

                                    </thead>
                                    <tbody id="rentalDate">
                                    </tbody>

                                </table>

                            </div>
                            <!-- /.box-body -->
                        </div>
                        <div class="text-right">
                            @*<button type="submit" class="btn btn-primary" id="generateRentDetails">Generate</button>*@
                            <button type="submit" class="btn btn-info" id="submitRentDiscount">Submit</button>
                            <button class="btn btn-sm btn-primary " id="updateRentDiscount">
                                <i class="ace-icon fa fa-check"></i> Update
                            </button>
                        </div>
                    </fieldset>
                </div>
            </div>

        </div>


    </div>

