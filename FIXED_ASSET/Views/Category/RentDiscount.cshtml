                            <th>Id</th>
                            <th>Project</th>
                            @*<th class="hidden"></th>*@
                            <th>Unit</th>
                            <th>Agreement</th>
                            <th>Start Date</th>
                            <th>End Date</th>
                            <th>Discount Type</th>
                            <th>Rent Discount Amount</th>
                            <th>Advance Calculate Type</th>
                            <th>Advance Discount Amount</th>
                            <th>AIT Calculate Type</th>
                            <th>AIT Discount Amount</th>
