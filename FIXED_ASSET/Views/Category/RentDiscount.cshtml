</section>

<!-- Main content -->
<section class="content">
    <!-- Default box -->
    <div class="center-block col-md-6" style="float: none;">    </div>
    <div class="box box-primary addnew">
        <div class="box-header with-border">
            <h3 class="box-title">@ViewBag.Title</h3>
            <div class="box-tools pull-right">
                <button class="btn btn-primary bg-blue-gradient hide-form">Hide</button>
            </div>
        </div>

        <form role="form"></form>
        <div class="box-body">
            <div class="panel panel-primary col-md-12 center-block" style="float: none;">
                <div class="panel-body">
                    <fieldset>
                        <legend>Rent Discount</legend>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-4">
                                    <label for="ProjectList">Project/Outlet</label>
                                    <select type="text" class="form-control select2 " style="width:290px;" id="ProjectList"></select>
                                    @*<select type="text" class="form-control select2 " style="width:315px;" id="ProjectList"></select>*@
                                </div>
                                <div class="col-md-4">
                                    <label for="UnitList">Unit</label>
                                    <input type="hidden" class="form-control" id="UnitList_edit" placeholder="">
                                    @*<input type="hidden" class="form-control" id="UnitList_edit" placeholder="">*@
                                    <select type="text" class="form-control select2 " style="width:290px;" id="UnitList" data-placeholder="Unit Name"></select>
                                </div>
                                <div class="col-md-4">
                                    <label for="AgreementList">Agreement</label>
                                    <input type="hidden" class="form-control" id="AgreementList_edit" placeholder="">
                                    <select type="text" class="form-control select2 " style="width:290px;" id="AgreementList" data-placeholder="Agreement"></select>
                                    @*<select type="text" class="form-control select2 " style="width:315px;" id="ProjectList"></select>*@
                                </div>

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-4">
                                    <label for="discountId">Discount ID</label>
                                    <input type="text" class="form-control col-sm-4" id="discountId" placeholder="Discount ID">
                                </div>
                                <div class="col-md-4">
                                    <label for="discount_Rent_Start_Date">Start Date</label>
                                    <div class="input-group date" data-provide="datepicker">
                                        <input type="text" data-date-format="mm/dd/yyyy" id="discount_Rent_Start_Date" class="form-control datepicker" placeholder="Select Date">
                                        <div class="input-group-addon">
                                            <span class="glyphicon glyphicon-th"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <label for="discount_Rent_End_Date">End Date</label>
                                    <div class="input-group date" data-provide="datepicker">
                                        <input type="text" data-date-format="mm/dd/yyyy" id="discount_Rent_End_Date" class="form-control datepicker" placeholder="Select Date">
                                        <div class="input-group-addon">
                                            <span class="glyphicon glyphicon-th"></span>
                                        </div>
                                    </div>
                                  </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-4">
                                    <label for="Rent_Discount_Type">Discount Type</label>
                                    <select id="rent_Discount_Type" class="form-control" name="Rent_Discount_Type">
                                        <option value="1">Percentage</option>
                                        <option value="2">Amount</option>
                                    </select>
                                </div>
                                <div class="col-md-4">
                                    <label for="rent_Amount">Rent Amount</label>
                                    <input type="text" class="form-control" id="rent_Amount" value="0" placeholder="Rent Amount/Percentage">
                                </div>
                                <div class="col-md-4">
                                    <label for="advance_Calculate_Type">Advance Calculate Type</label>
                                    <select id="advance_Calculate_Type" class="form-control" name="Advance Calculate Type">
                                        <option value="1">Percentage</option>
                                        <option value="2">Amount</option>
                                        <option value="2">None</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-4">
                                    <label for="advance_Amount">Advance Amount</label>
                                    <input type="text" class="form-control" id="advance_Amount" value="0" placeholder="Advance Amount/Percentage">
                                </div>
                                <div class="col-md-4">
                                    <label for="aIT_Calculate_Type">AIT Calculate Type</label>
                                    <select id="aIT_Calculate_Type" class="form-control" name="AIT Calculate Type">
                                        <option value="1">Percentage</option>
                                        <option value="2">Amount</option>
                                        <option value="2">None</option>
                                    </select>
                                </div>
                                <div class="col-md-4">
                                    <label for="aIT_Amount">AIT Amount</label>
                                    <input type="text" class="form-control" id="aIT_Amount" value="0" placeholder="AIT Amount">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                             <div class="col-md-4">
                                    <label for="">Status</label>
                                    <select id="active_status" class="form-control" name="active_status">
                                        <option value="true">Active</option>
                                        <option value="false">InActive</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                        </div>

                        <div class="col-md-12">
                            <div class="box-body table-responsive no-padding">
                                <table id="slotdatatable" class="table table-bordered table-striped compact hover" cellspacing="0" width="100%">
