                        });
                        $("#ProjectList").val("");
                        $("#UnitList").val("");
                        $("#AgreementList").val("");
                        // $("#active_status").val(-1);
                        $('.addnew').show();
                        setTimeout(function () {
                            location.reload();
                        }, 5000);
                        // $('#datatable').load(document.URL + ' #datatable');

                    }

