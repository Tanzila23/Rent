                    aIT_Calculate_Type: aIT_Calculate_Type,
                    aIT_Amount: aIT_Amount,
                    activeStatus: activeStatus

                }),
                //success: function (result) {
                //    location.reload(true);

                //    $("h2").css({ color: result[1] });
                //},
                error: function () {
                    alert("error");
                }
            });
        }

    });
    $("#updateRentDiscount").click(function UpdateData() {

        // var rowStatus = 2;

