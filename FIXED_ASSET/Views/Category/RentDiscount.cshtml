                              //result += "<option selected id='" + response[i].UNIT_ID + "'  data-STORAGE_ID='" + response[i].EXTERNAL_ID + "' title='" + response[i].UNIT_NAME + "'value='" + response[i].UNIT_ID + "'>" + response[i].UNIT_NAME + "</option>";
                        }
                         else {
                              result += "<option id='" + response[i].Agreement_ID + "'+ value='" + response[i].External_ID + "' >" + response[i].External_ID + "</option>";
                         }
                       

                    }
                }
                $('select#AgreementList').append(result);


            }
        });
    }

    function getData() {

        $('#datatable').DataTable({
            "searching": true,
            "paging": true,
            "ordering": true,
            "info": true,
            "language":
            {
                "processing": "<div class='overlay custom-loader-background'><i class='fa fa-cog fa-spin custom-loader-color'></i></div>"
            },
            "processing": true,
            "ajax": {
                "url": "/AssetCategory/RentDiscountList",
                "type": "GET",
                "datatype": "json"
            },
            "columns": [
                { "data": "External_ID", "autoWidth": true },
                { "data": "Project_Name", "autoWidth": true },
                { "data": "UNIT_NAME", "autoWidth": true },
                 { "data": "Agreement_ID", "autoWidth": true },
                {
                    "data": "Start_DT", "autoWidth": true,
                  render: function(d) {
                  return moment(d).format("DD-MM-YYYY");
                    }
                },
                {
                    "data": "End_DT", "autoWidth": true,
                      render: function(d) {
                  return moment(d).format("DD-MM-YYYY");
                    }
                },
                { "data": "Rent_Disc_Type", "autoWidth": true },
                 { "data": "Rent_Disc_Amount", "autoWidth": true },
                { "data": "Adv_Adj_Type", "autoWidth": true },
                { "data": "Adv_Adj_Amount", "autoWidth": true },
                { "data": "AIT_CalType", "autoWidth": true },
                  { "data": "AIT_Amount", "autoWidth": true },
                { "data": "Status", "autoWidth": true },
                {
                    "data": "Discount_ID", "width": "50px", "render": function (data) {
                        return '<button type="button" class="btn btn-primary" onclick="editRentDiscount(' + data + ')">Edit</button>';
                        // return '<div class="btn-group"><button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button><ul class="dropdown-menu"><li><button type="button" class="btn btn-primary" onclick="getbyID(' + data + ')">DETAIL</button></li><li><button type="button" class="btn btn-info" onclick="getOldCategory(' + data + ')"> OLD CATEGORY</button></li><li><button type="button" class="btn btn-info" onclick="getbyInventoryInsert(' + data + ')">INVENTORY INSERT</button></li><li><button type="button" class="btn btn-warning" onclick="getbyInventoryView(' + data+')">INVENTORY VIEW</button></li></ul></div>';
                    }
                }
            ]
        });

    }

    function editRentDiscount(discountId) {
        $("#External_ID").prop("disabled", true);
        // $("#ClassList").prop("disabled", true);
        $('.addnew').show();
        $('#updateRentDiscount').show();
        $('#submitRentDiscount').hide();
          $('#submitAgreementIncrement').hide();

        $.ajax({
            type: "POST",
            url: "/AssetCategory/Edit_RentDiscount",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            data: JSON.stringify({
                //  obj: asd
                discountId: discountId

            }),
            success: function (response) {

                console.log(response);
                console.log(response.data);


                $("#discountId").val(response.data.External_ID);
                $("#ProjectList").val(response.data.Project_ID).trigger('change');
                $("#UnitList_edit").val(response.data.Unit_ID).trigger('change');
                AgreementistListReload(response.data.Unit_ID, response.data.Agreement_ID);
               // alert(response.data.Unit_ID, response.data.Agreement_ID);
              //  $("#AgreementList_edit").val(response.data.Agreement_ID);
                $("#discount_Rent_Start_Date").val(response.data.Start_DT);
                $("#discount_Rent_End_Date").val(response.data.End_DT);
                $("#rent_Discount_Type").val(response.data.Rent_Disc_Type);
                $("#rent_Amount").val(response.data.Rent_Disc_Amount);
                $("#advance_Calculate_Type").val(response.data.Adv_Adj_Type);
                $("#advance_Amount").val(response.data.Adv_Adj_Amount);
                $("#aIT_Calculate_Type").val(response.data.AIT_CalType);
                $("#aIT_Amount").val(response.data.AIT_Amount);
                if (response.data.Status == 'Active') {
                    $("#active_status").val('true');
                } else {
                    $("#active_status").val('false');
                }

                $("#btnUpdate").show();

